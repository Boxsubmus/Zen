// Match keyword, stolen from Rust mostly.

public function void main()
{
    let age = 18;
    match age
    {
        (value < 0) => println("huh?") // This will not work
        (0..17) => println("too young..."),
        (18 | 19 | 20 | 21 | 22 | 23 | 24 | 25 | 26) => println("hello"),
        _ => println("too old")
    }
}